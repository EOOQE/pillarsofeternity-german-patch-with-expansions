      <ID>1805</ID>
      <DefaultText>Säureatem</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>1806</ID>
      <DefaultText>Peitschenschweif</DefaultText>
      <FemaleText />
    </Entry>
