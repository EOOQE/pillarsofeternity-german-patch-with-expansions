      <ID>1952</ID>
      <DefaultText>Trank der Gnadenfrist</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>1953</ID>
      <DefaultText>Trank des Flammenschilds</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>1954</ID>
      <DefaultText>Trank der Geringen arkanen Reflexion</DefaultText>
      <FemaleText />
