      <ID>1963</ID>
      <DefaultText>Sturmrufer</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>1964</ID>
      <DefaultText>Nachtumhang</DefaultText>
      <FemaleText />
    </Entry>
