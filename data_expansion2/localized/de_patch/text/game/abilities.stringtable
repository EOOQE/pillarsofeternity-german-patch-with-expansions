      <DefaultText>Beschwört mehrere zufällige Gefäße.</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>1611</ID>
      <DefaultText>Verwirren</DefaultText>
      <FemaleText />
